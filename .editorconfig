# Editor configuration, see https://editorconfig.org/
root = true

[*]
charset = utf-8
end_of_line = lf
indent_size = 2
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true

[Makefile]
indent_size = 4
indent_style = tab

[*.md]
trim_trailing_whitespace = false

# 自動生成コードは対象外とする
[migration_lock.toml]
insert_final_newline = false
